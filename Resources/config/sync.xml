<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="false" />
        <service id="emsch.command.sync.pull" class="EMS\ClientHelperBundle\Command\Sync\PullCommand">
            <argument type="service" id="emsch.sync.pull_helper" />
            <tag name="console.command" />
        </service>

        <service id="emsch.sync.pull_helper" class="EMS\ClientHelperBundle\Helper\Sync\PullHelper">
        </service>
    </services>
</container>